void printSets(vector<string> a, vector<string> b) {
    cout << "Set A = {";
    if (a.empty() == true) {
        cout << "";
    }
    else {
        for (int i = a.size(); i > 0; i--) {
            cout << a[i] << ",";
        }
    }
    cout << "}";
}
