void printSets(vector<string> a, vector<string> b) {
    cout << "Set A = {";
    if (a.empty() == true) {
        cout << "";
    }
    else {
        cout << "{";
        for(auto element: a){
            cout << element;
        }
        cout << "}";
    }
    cout << "}\n";
    
    cout << "Set B = {";
    if (b.empty() == true) {
        cout << "";
    }
    else {
        cout << "{";
        for(auto element: b){
            cout << element;
        }
        cout << "}";
    }
    cout << "}\n\n";
}



