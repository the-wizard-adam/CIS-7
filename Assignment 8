#include <iostream>

using namespace std;

bool isPrime(int);

int main()
{
    int num = 1000;
    int count = 0;
    bool numArray[num] = {false};
    
    for (int i = num; i > 1; i--) {
        numArray[i] = isPrime(i);
        if (numArray[i] == true) {
            count++;
        }
        else {
        }
    }
    
    cout << count;

    return 0;
}

bool isPrime(int i) {
    for (int j = (i-1); j > 1; j--) {
        if (i % j == 0) {
            return false;
        }
        else {
        }
    }
    return true;
}
